// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        // "node_modules": true,
        ".vscode": true
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "git.ignoreLimitWarning": true
}